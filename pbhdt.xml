<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" 
        xmlns:h="http://www.w3.org/1999/xhtml" 
        xmlns:jr="http://openrosa.org/javarosa" 
        xmlns:orx="http://openrosa.org/xforms" 
        xmlns:odk="http://www.opendatakit.org/xforms"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <h:head>
        <h:title>Pediatric Behavioral Health Digital Toolkit Screening Measures</h:title>
        <model>
			<itext>
				<translation default="true()" lang="English">
					<text id="yes">
						<value>Yes</value>
					</text>
					<text id="no">
						<value>No</value>
					</text>
					<text id="pic1">
						<value form="image">jr://images/pic1.jpg</value>
					</text>
					<text id="pic2">
						<value form="image">jr://images/pic2.jpg</value>
					</text>
					<text id="pic3">
						<value form="image">jr://images/pic2.jpg</value>
					</text>
					<text id="developmenttree">
						<value form="image">jr://images/developmenttree.jpg</value>
					</text>
				</translation>
				<translation lang="Luganda">
					
				</translation>
			
			</itext>
			
            <instance>
                <data id="P-BHDT">
					<eligibility>
						<E1/>
						<E2/>
						<E3/>
					</eligibility>
					
					<demographic>
						<D1/>
						<D2/>
						<D3/>
						<D4years/>
						<D4months/>
						<D5/>
						<D6/>
						<D7/>
						<D8/>
						<D9/>
						<D10/>
						<D10specify/>
						<D11/>
						<D11specify/>
						<D12/>
						<D13/>
						<D13specify/>
					</demographic>
					<social-development>
						<social_development_discussion>
							<picture1/>
							<picture2/>
							<picture3/>
						</social_development_discussion>
						<social_development_description/>
						<empathic-Behaviors>
							<empathic-description/>
							<empathic-header/>
							<EB1/>
							<EB2/>
							<EB3/>
							<EB4/>
							<EB5/>
						</empathic-Behaviors>
						<pear-relationship>
							<PR1/>
							<PR2/>
							<PR3/>
							<PR4/>
						</pear-relationship>
						<family-relationship>
							<FR1/>
							<FR2/>
							<FR3/>
							<FR4/>
							<FR5/>
							<FR6/>
							<FR7/>
							<FR8/>
						</family-relationship>
					</social-development>
					<child-emotions>	
						<anger>
							<CHA1/>
							<CHA2/>
							<CHA3/>
							<CHA4/>
							<CHA5/>
							<CHA6/>
							<CHA7/>
							<CHA8/>
							<CHA9/>					
						</anger>
						<fear-over-anxious>
							<FAO1/>
							<FAO2/>
							<FAO3/>
							<FAO4/>
							<FAO5/>
							<FAO6/>
						</fear-over-anxious>
						<sadness>
							<CES1/>
							<CES2/>
							<CES3/>
							<CES4/>
							<CES5/>
							<CES6/>
							<CES7/>
						</sadness>
					
					</child-emotions>
					
					<meta>
						<DeviceID/>
						<StartTime/>
						<EndTime/>
						<instanceID/>
						<SubscriberID/>
						<phoneNumber/>
					</meta>
					
					
					
				</data>
            </instance>
            <bind nodeset="/data/meta/instanceID" preload="uid" type="xsd:string"/>
			<bind nodeset="/data/meta/StartTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="start"/>
			<bind nodeset="/data/meta/EndTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="end"/>
			<bind nodeset="/data/meta/DeviceID" type="string" jr:preload="property" jr:preloadParams="deviceid"/>
			<bind nodeset="/data/meta/SubscriberID" type="string" jr:preload="property" jr:preloadParams="subscriberid"/>
			<bind nodeset="/data/meta/phoneNumber" type="string" jr:preload="property" jr:preloadParams="phone number"/>
			
			<bind nodeset="/data/eligibility/E1" type="int" required="true()"/>
			<bind nodeset="/data/eligibility/E2" required="true()" relevant="/data/eligibility/E1 &gt;= 18" />
			<bind nodeset="/data/eligibility/E3" required="true()" relevant="/data/eligibility/E1 &gt;= 18 and /data/eligibility/E2 = 1 " />
			
			<bind nodeset="/data/demographic" relevant="/data/eligibility/E1 &gt;= 18 and /data/eligibility/E2 = 1 " />
			<bind nodeset="/data/demographic/D2" type="int" />
			<bind nodeset="/data/demographic/D3" type="int" />
			<bind nodeset="/data/demographic/D4years" type="int" constraint=". &gt; 0 " jr:constraintMsg="Child’s years can not be 0"/>
			<bind nodeset="/data/demographic/D4months" type="int" constraint=". &gt; 0 " jr:constraintMsg="Child’s Months can not be 0"/>
			<bind nodeset="/data/demographic/D5" />
			<bind nodeset="/data/demographic/D6" />
			<bind nodeset="/data/demographic/D7" type="int"/>
			<bind nodeset="/data/demographic/D8" type="int"/>
			<bind nodeset="/data/demographic/D10" />
			<bind nodeset="/data/demographic/D10specify" relevant="/data/demographic/D10 = 4" />
			<bind nodeset="/data/demographic/D11" />
			<bind nodeset="/data/demographic/D11specify" relevant="/data/demographic/D11 = 4"/>
			<bind nodeset="/data/demographic/D12" />
			<bind nodeset="/data/demographic/D13" />
			<bind nodeset="/data/demographic/D13specify" relevant="/data/dedication/D13 = 3"/>
			
			<bind nodeset="/data/social-development" relevant="/data/eligibility/E1 &gt;= 18 and /data/eligibility/E2 = 1 " />
			<bind nodeset="/data/social-development/social_development_discussion/picture1" readonly="true()" />
			<bind nodeset="/data/social-development/social_development_discussion/picture2" readonly="true()" />
			<bind nodeset="/data/social-development/social_development_discussion/picture3" readonly="true()" />
			<bind nodeset="/data/social-development/social_development_description" readonly="true()"/>
			<bind nodeset="/data/social-development/empathic-Behaviors/EB1" />
			<bind nodeset="/data/social-development/empathic-Behaviors/empathic-description" readonly="true()" />
			<bind nodeset="/data/social-development/empathic-Behaviors/EB2" />
			<bind nodeset="/data/social-development/empathic-Behaviors/EB3" />
			<bind nodeset="/data/social-development/empathic-Behaviors/EB4" />
			<bind nodeset="/data/social-development/empathic-Behaviors/EB5" />
						
			<bind nodeset="/data/social-development/pear-relationship" />
			
			<bind nodeset="/data/social-development/family-relationship" />
			
			
			<bind nodeset="/data/child-emotions" relevant="/data/eligibility/E1 &gt;= 18 and /data/eligibility/E2 = 1 " />
			<bind nodeset="/data/child-emotions/anger"/>
			<bind nodeset="/data/child-emotions/fear-over-anxious" />
			<bind nodeset="/data/child-emotions/sadness" />
			
			
        </model>
    </h:head>
    <h:body>
		<!-- Eligibility Screening -->
		<group ref="/data/eligibility" appearance="field-list">
			<input ref="E1">
				<label>Your Age</label>
				<hint>If less than 18 years old; Stop here; Only greater than or equall to 18 years of age is qualified"></hint>
			</input>
			
			<select1 ref="E2">
				<label>Are you the primary caregiver and living with the child?"</label>
				<hint>If NO,  STOP"></hint>
				<item>
					<label ref="jr:itext('yes')"></label>
					<value>1</value>
				</item>
				<item>
					<label ref="jr:itext('no')"></label>
					<value>0</value>
				</item>
			</select1>
			<select1 ref="E3">
			<label>Is this the first time you are signing up for using the Digital Toolkit?</label>
				<item>
					<label ref="jr:itext('yes')"></label>
					<value>1</value>
				</item>
				<item>
					<label ref="jr:itext('no')"></label>
					<value>0</value>
				</item>
			</select1>
		</group>
        
		
		<!-- Demographic -->
		<group ref="/data/demographic" appearance="field-list">
		
			<input ref="D1">
				<label>FBO ID:</label>
			</input>
			<input ref="D2">
				<label>Family ID:</label>
			</input>
			<input ref="D3">
				<label>Child ID:</label>
			</input>
			<input ref="D4years">
				<label>Child’s Age in Years:</label>
			</input>
			<input ref="D4months">
				<label>Child’s Age in Months:</label>
			</input>
			
			<select1 ref="D5">
				<label>Attends school or early childhood care:</label>
				<item>
					<label ref="jr:itext('yes')"></label>
					<value>1</value>
				</item>
				<item>
					<label ref="jr:itext('no')"></label>
					<value>0</value>
				</item>
			</select1>
			<input ref="D6">
				<label>If in primary school, which grade:</label>
			</input>
			<input ref="D7">
				<label>Number of children at home:</label>
			</input> 
			<input ref="D8">
				<label>Number of adults at home:</label>
			</input>  
			<select1 ref="D9">
				<label>Your gender:</label>
				<item>
					<label>Male</label>
					<value>1</value>
				</item>
				<item>
					<label>Female</label>
					<value>0</value>
				</item>
			</select1>
			<select1 ref="D10">
				<label>Your relationship with the child:</label>
				<item>
					<label>Mother</label>
					<value>1</value>
				</item>
				<item>
					<label>Father</label>
					<value>2</value>
				</item>
				<item>
					<label>Grandparent</label>
					<value>3</value>
				</item>
				<item>
					<label>Other</label>
					<value>4</value>
				</item>
			</select1>
			<input ref="D10specify" >
				<label>Specify other relationship with the child</label>
			</input>
			<select1 ref="D11">
				<label>What is Your Religion:</label>
				<item>
					<label>No religion</label>
					<value>0</value>
				</item>
				<item>
					<label>Muslim</label>
					<value>1</value>
				</item>
				<item>
					<label>Christian</label>
					<value>2</value>
				</item>
				<item>
					<label>Catholic</label>
					<value>3</value>
				</item>
				<item>
					<label>Other religion </label>
					<value>4</value>
				</item>
			</select1>
			<input ref="D11specify" >
				<label>Specify other religion</label>
			</input>
			<select1 ref="D12">
				<label>Your highest education:</label>
				<item>
					<label>None</label>
					<value>0</value>
				</item>
				<item>
					<label>Primary</label>
					<value>1</value>
				</item>
				<item>
					<label>O’level</label>
					<value>2</value>
				</item>
				<item>
					<label>A’level</label>
					<value>3</value>
				</item>
				<item>
					<label>Tertiary </label>
					<value>4</value>
				</item>
				<item>
					<label>Non Standard Curriculum </label>
					<value>5</value>
				</item>
			</select1>
			<select1 ref="D13">
				<label>Marital Status:</label>
				<item>
					<label>Single (Single/Widow/or Divorced) </label>
					<value>0</value>
				</item>
				<item>
					<label>Married</label>
					<value>1</value>
				</item>
				<item>
					<label>Not married but live with a partner</label>
					<value>2</value>
				</item>
				<item>
					<label>other</label>
					<value>3</value>
				</item>
				</select1>
			<input ref="D13specify" >
				<label>Specify other Marital status</label>
			</input>
		</group>
		
		<!-- Child Social Development -->
		<group ref="/data/social-development">
			<group ref="social_development_discussion" appearance="field-list">
				<label>In today’s discussion, we will talk about……. your child’s behaviors and relationship; your parenting approaches and strategies; and your family relationship and resources to support your child/children. </label>
				<!-- pictures to display -->
				<input ref="picture1">
					<hint>Child’s behaviors and relationship  </hint>
					<label ref="jr:itext('pic1')"></label>
				</input>
				<input ref="picture2">
					<hint>Parenting approaches</hint>
					<label ref="jr:itext('pic1')"></label>
				</input>
				<input ref="picture3">
					<hint>Family relationship and resources </hint>
					<label ref="jr:itext('pic1')"></label>
				</input>
			</group>
			<input ref="social_development_description">
				<hint>A child’s growth is like a tree. There are five areas of child development (including social, emotional, behavioral, physical, and academic/learning). To support a strong tree, the child needs to have adequate nutrition, which is usually provided by parents and family members. All the areas listed in this picture are inter-related. For this part of the interview, I am going to ask you questions from each area. We will look at the results together after you have answered all the questions</hint>
				<label ref="jr:itext('developmenttree')"></label>
				
			</input>
			<group ref="empathic-Behaviors" appearance="field-list">
				<input ref="empathic-description">
					<hint>In the past month, please describe……….</hint>
					<label>1=never, 2=rarely, 3=sometimes, 4=usually, 5=always</label>
				</input>
				<select1 appearance="label" ref="empathic-header">
					<label></label>
					<item><label>never</label><value>1</value></item>
					<item><label>rarely</label><value>2</value></item>
					<item><label>sometimes</label><value>3</value></item>
					<item><label>usually</label><value>4</value></item>
					<item><label>always</label><value>5</value></item>
				</select1>
				
				<select1 appearance="list-nolabel" ref="EB1">
					<label>How often has your child tried to stop a fight or argument </label>
					<item>
						<label>never</label>
						<value>1</value>
					</item>
					<item>
						<label>rarely</label>
						<value>2</value>
					</item>
					<item>
						<label>sometimes</label>
						<value>3</value>
					</item>
					
					<item>
						<label>usually</label>
						<value>4</value>
					</item>
					<item>
						<label>always</label>
						<value>5</value>
					</item>
				</select1>
			
				<select1 appearance="list-nolabel" ref="EB2">
					<label>How often has your child shared candies and extra food  </label>
					<item>
						<label>never</label>
						<value>1</value>
					</item>
					<item>
						<label>rarely</label>
						<value>2</value>
					</item>
					<item>
						<label>sometimes</label>
						<value>3</value>
					</item>
					<item>
						<label>usually</label>
						<value>4</value>
					</item>
					<item>
						<label>always</label>
						<value>5</value>
					</item>
				</select1>
				
				<select1 appearance="list-nolabel" ref="EB3">
					<label>How often has your child helped other children who are feeling sick </label>
					<item>
						<label>never</label>
						<value>1</value>
					</item>
					<item>
						<label>rarely</label>
						<value>2</value>
					</item>
					<item>
						<label>sometimes</label>
						<value>3</value>
					</item>
					<item>
						<label>usually</label>
						<value>4</value>
					</item>
					<item>
						<label>always</label>
						<value>5</value>
					</item>
				</select1>
				
				<select1 appearance="list-nolabel" ref="EB4">
					<label>How often has your child comforted a child who is crying or upset  </label>
					<item>
						<label>never</label>
						<value>1</value>
					</item>
					<item>
						<label>rarely</label>
						<value>2</value>
					</item>
					<item>
						<label>sometimes</label>
						<value>3</value>
					</item>
					<item>
						<label>usually</label>
						<value>4</value>
					</item>
					<item>
						<label>always</label>
						<value>5</value>
					</item>
				</select1>
			
				<select1 appearance="list-nolabel" ref="EB5">
					<label>How often has your child clapped or smiled if someone else does something well  </label>
					<item>
						<label>never</label>
						<value>1</value>
					</item>
					<item>
						<label>rarely</label>
						<value>2</value>
					</item>
					<item>
						<label>sometimes</label>
						<value>3</value>
					</item>
					<item>
						<label>usually</label>
						<value>4</value>
					</item>
					<item>
						<label>always</label>
						<value>5</value>
					</item>
				</select1>
			</group>
			<!-- pear-relationship -->
			<group ref="pear-relationship">
				<label>In the past month, please describe……….</label>
				<hint>1=never, 2=rarely, 3=sometimes, 4=usually, 5=always</hint>
				<select1 ref="PR1">
					<label>How often your child has been left out by other children </label>
					<item>
						<label>never</label>
						<value>1</value>
					</item>
					<item>
						<label>rarely</label>
						<value>2</value>
					</item>
					<item>
						<label>sometimes</label>
						<value>3</value>
					</item>
					<item>
						<label>usually</label>
						<value>4</value>
					</item>
					<item>
						<label>always</label>
						<value>5</value>
					</item>
				</select1>
				
				<select1 ref="PR2">
					<label>How often other children have refused to let your child play with them </label>
					<item>
						<label>never</label>
						<value>1</value>
					</item>
					<item>
						<label>rarely</label>
						<value>2</value>
					</item>
					<item>
						<label>sometimes</label>
						<value>3</value>
					</item>
					<item>
						<label>usually</label>
						<value>4</value>
					</item>
					<item>
						<label>always</label>
						<value>5</value>
					</item>
				</select1>
				
				<select1 ref="PR3">
					<label>How often your child has been teased and made fun of by other children  </label>
					<item>
						<label>never</label>
						<value>1</value>
					</item>
					<item>
						<label>rarely</label>
						<value>2</value>
					</item>
					<item>
						<label>sometimes</label>
						<value>3</value>
					</item>
					<item>
						<label>usually</label>
						<value>4</value>
					</item>
					<item>
						<label>always</label>
						<value>5</value>
					</item>
				</select1>
				
				<select1 ref="PR4">
					<label>How often your child has been pushed or shoved around by other children </label>
					<item>
						<label>never</label>
						<value>1</value>
					</item>
					<item>
						<label>rarely</label>
						<value>2</value>
					</item>
					<item>
						<label>sometimes</label>
						<value>3</value>
					</item>
					<item>
						<label>usually</label>
						<value>4</value>
					</item>
					<item>
						<label>always</label>
						<value>5</value>
					</item>
				</select1>
			</group> 
			<!-- family-relationship  -->
			<group ref="family-relationship">
				<label>For each of the following statement, please tell me how much it is like your child…</label>
				<hint>0=Does not do this at all, 1= Does this a little, 2= Does this quite a bit, 3=Does this a lot</hint>
				<select1 ref="FR1">
					<label>Participates in family activities </label>
					<item>
						<label>Does not do this at all</label>
						<value>0</value>
					</item>
					<item>
						<label>Does this a little</label>
						<value>1</value>
					</item>
					<item>
						<label>Does this quite a bit</label>
						<value>2</value>
					</item>
					<item>
						<label>Does this a lot</label>
						<value>3</value>
					</item>
					</select1>
					
					
				<select1 ref="FR2">
					<label>Complies with rules at home </label>
					<item>
						<label>Does not do this at all</label>
						<value>0</value>
					</item>
					<item>
						<label>Does this a little</label>
						<value>1</value>
					</item>
					<item>
						<label>Does this quite a bit</label>
						<value>2</value>
					</item>
					<item>
						<label>Does this a lot</label>
						<value>3</value>
					</item>
					</select1>
							
				<select1 ref="FR3">
					<label>Participates in church or religion activities </label>
					<item>
						<label>Does not do this at all</label>
						<value>0</value>
					</item>
					<item>
						<label>Does this a little</label>
						<value>1</value>
					</item>
					<item>
						<label>Does this quite a bit</label>
						<value>2</value>
					</item>
					<item>
						<label>Does this a lot</label>
						<value>3</value>
					</item>
				</select1>
				<select1 ref="FR4">
					<label>Interacts positively with siblings</label>
					<item>
						<label>Does not do this at all</label>
						<value>0</value>
					</item>
					<item>
						<label>Does this a little</label>
						<value>1</value>
					</item>
					<item>
						<label>Does this quite a bit</label>
						<value>2</value>
					</item>
					<item>
						<label>Does this a lot</label>
						<value>3</value>
					</item>
				</select1>
				<select1 ref="FR5">
					<label>Interacts positively with parents </label>
					<item>
						<label>Does not do this at all</label>
						<value>0</value>
					</item>
