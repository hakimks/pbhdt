<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" 
        xmlns:h="http://www.w3.org/1999/xhtml" 
        xmlns:jr="http://openrosa.org/javarosa" 
        xmlns:orx="http://openrosa.org/xforms" 
        xmlns:odk="http://www.opendatakit.org/xforms"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <h:head>
        <h:title>Pediatric Behavioral Health Digital Toolkit Screening Measures</h:title>
        <model>
			<itext>
				<translation default=true() lang="English">
					<text id="yes">
						<value>Yes</value>
					</text>
					<text id="no">
						<value>No</value>
					</text>
					<text id="pic1">
						<value form="image">jr://images/pic1.jpg</value>
					</text>
					<text id="pic2">
						<value form="image">jr://images/pic2.jpg</value>
					</text>
					<text id="pic3">
						<value form="image">jr://images/pic2.jpg</value>
					</text>
				</translation>
				<translation lang="Luganda">
					
				</translation>
			
			</itext>
			
            <instance>
                <data id="P-BHDT">
					<eligibility>
						<E1/>
						<E2/>
						<E3/>
					</eligibility>
					
					<demographic>
						<D1/>
						<D2/>
						<D3/>
						<D4years/>
						<D4months/>
						<D5/>
						<D6/>
						<D7/>
						<D8/>
						<D9/>
						<D10/>
						<D10specify/>
						<D11/>
						<D11specify/>
						<D12/>
						<D13/>
						<D13specify/>
					</demographic>
                    
					<empathic-Behaviors>
						<picture1/>
						<picture2/>
						<picture3/>
						<q3a/>
						<q3b/>
						<q3c/>
						<q3d/>
						<q3e/>
						<q3f/>
						<q3g/>
						<q3h/>
						<q3i/>
						<q3j/>
					</empathic-Behaviors>
					
					<pear-relationship>
						<q4a/>
						<q4b/>
						<q4c/>
						<q4d/>
						<q4e/>
						<q4f/>
						<q4g/>
						<q4h/>
						<q4i/>
					</pear-relationship>	
						
					<family-relationship>
						<q5a/>
						<q5b/>
						<q5c/>
						<q5d/>
						<q5e/>
						<q5f/>
						<q5g/>
						<q5h/>
						<q5i/>
						<q5j/>
					</family-relationship>			

					<anger >
					<q6a/>
					<q6b/>
					<q6c/>
					<q6d/>
					<q6e/>
					<q6f/>
					<q6g/>
					<q6h/>
					<q6i/>					
					</anger>
					<meta>
						<DeviceID/>
						<StartTime/>
						<EndTime/>
						<instanceID/>
						<SubscriberID/>
						<phoneNumber/>
					</meta>
					
					
					
				</data>
            </instance>
            <bind nodeset="/data/meta/instanceID" preload="uid" type="xsd:string"/>
			<bind nodeset="/data/meta/StartTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="start"/>
			<bind nodeset="/data/meta/EndTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="end"/>
			<bind nodeset="/data/meta/DeviceID" type="string" jr:preload="property" jr:preloadParams="deviceid"/>
			<bind nodeset="/data/meta/SubscriberID" type="string" jr:preload="property" jr:preloadParams="subscriberid"/>
			<bind nodeset="/data/meta/phoneNumber" type="string" jr:preload="property" jr:preloadParams="phone number"/>
			
			<bind nodeset="/data/eligibility/E1" type="int" required="true()"/>
			<bind nodeset="/data/eligibility/E2" required="true()" relevant="/data/eligibility/E1 &gt;= 18" />
			<bind nodeset="/data/eligibility/E3" required="true()" relevant="/data/eligibility/E1 &gt;= 18 and /data/eligibility/E2 = 1 " />
			
			<bind nodeset="/data/demographic" relevant="/data/eligibility/E1 &gt;= 18 and /data/eligibility/E2 = 1 " />
			<bind nodeset="/data/demographic/D2" type="int" />
			<bind nodeset="/data/demographic/D3" type="int" />
			<bind nodeset="/data/demographic/D4years" type="int" constraint=". &gt; 0 " jr:constraintMsg="Child’s years can not be 0"/>
			<bind nodeset="/data/demographic/D4months" type="int" constraint=". &gt; 0 " jr:constraintMsg="Child’s Months can not be 0"/>
			<bind nodeset="/data/demographic/D5" />
			<bind nodeset="/data/demographic/D6" />
			<bind nodeset="/data/demographic/D7" type="int"/>
			<bind nodeset="/data/demographic/D8" type="int"/>
			<bind nodeset="/data/demographic/D10" />
			<bind nodeset="/data/demographic/D10specify" relevant="/data/demographic/D10 = 4" />
			<bind nodeset="/data/demographic/D11" />
			<bind nodeset="/data/demographic/D11specify" relevant="/data/demographic/D11 = 4"/>
			<bind nodeset="/data/demographic/D12" />
			<bind nodeset="/data/demographic/D13" />
			<bind nodeset="/data/demographic/D13specify" relevant="/data/dedication/D13 = 3"/>
			
			<bind nodeset="/data/empathic-Behaviors" relevant="/data/eligibility/E1 &gt;= 18 and /data/eligibility/E2 = 1 " />
			<bind nodeset="/data/empathic-Behaviors/picture1" readonly="true()" />
			<bind nodeset="/data/empathic-Behaviors/picture2" readonly="true()" />
			<bind nodeset="/data/empathic-Behaviors/picture3" readonly="true()" />
			<bind nodeset="/data/empathic-Behaviors/q3a" />
			<bind nodeset="/data/empathic-Behaviors/q3b" />
			<bind nodeset="/data/empathic-Behaviors/q3c" />
			<bind nodeset="/data/empathic-Behaviors/q3d" />
			<bind nodeset="/data/empathic-Behaviors/q3e" />
			<bind nodeset="/data/empathic-Behaviors/q3f" />
			<bind nodeset="/data/empathic-Behaviors/q3g" />
			<bind nodeset="/data/empathic-Behaviors/q3h" />
			<bind nodeset="/data/empathic-Behaviors/q3i" />
			<bind nodeset="/data/empathic-Behaviors/q3j" />
			
			<bind nodeset="/data/pear-relationship" relevant="/data/eligibility/E1 &gt;= 18 and /data/eligibility/E2 = 1 " />
			<bind nodeset="/data/pear-relationship/q4a" />
			<bind nodeset="/data/pear-relationship/q4b" />
			<bind nodeset="/data/pear-relationship/q4c" />
			<bind nodeset="/data/pear-relationship/q4d" />
			<bind nodeset="/data/pear-relationship/q4e" />
			<bind nodeset="/data/pear-relationship/q4f" />
			<bind nodeset="/data/pear-relationship/q4g" />
			<bind nodeset="/data/pear-relationship/q4h" />
			<bind nodeset="/data/pear-relationship/q4i" />
			
			<bind nodeset="/data/family-relationship" relevant="/data/eligibility/E1 &gt;= 18 and /data/eligibility/E2 = 1 " />
			<bind nodeset="/data/family-relationship/q5a" />
			<bind nodeset="/data/family-relationship/q5b" />
			<bind nodeset="/data/family-relationship/q5c" />
			<bind nodeset="/data/family-relationship/q5d" />
			<bind nodeset="/data/family-relationship/q5e" />
			<bind nodeset="/data/family-relationship/q5f" />
			<bind nodeset="/data/family-relationship/q5g" />
			<bind nodeset="/data/family-relationship/q5h" />
			<bind nodeset="/data/family-relationship/q5i" />
			<bind nodeset="/data/family-relationship/q5j" />
			
			<bind nodeset="/data/anger" relevant="/data/eligibility/E1 &gt;= 18 and /data/eligibility/E2 = 1 " />
			<bind nodeset="/data/anger/q6a" />
			<bind nodeset="/data/anger/q6b" />
			<bind nodeset="/data/anger/q6c" />
			<bind nodeset="/data/anger/q6d" />
			<bind nodeset="/data/anger/q6e" />
			<bind nodeset="/data/anger/q6f" />
			<bind nodeset="/data/anger/q6g" />
			<bind nodeset="/data/anger/q6i" />
			
			
        </model>
    </h:head>
    <h:body>
		<!-- Eligibility Screening -->
		<group ref="/data/eligibility">
			<input ref="E1">
				<label ref="Your Age"/>
				<hint ref="If <18 years old; Stop here; Only >=18 years of age is qualified"></hint>
			</input>
			
			<select1 ref="E2">
				<label ref="Are you the primary caregiver and living with the child?"></label>
				<hint ref="If NO--> STOP"></hint>
				<item>
					<label ref="jr:itext('yes')"></label>
					<value>1</value>
				</item>
				<item>
					<label ref="jr:itext('no')"></label>
					<value>0</value>
				</item>
			</select1>
			<select1 ref="E3">
			<label ref="Is this the first time you are signing up for using the Digital Toolkit? "></label>
				<item>
					<label ref="jr:itext('yes')"></label>
					<value>1</value>
				</item>
				<item>
					<label ref="jr:itext('no')"></label>
					<value>0</value>
				</item>
			</select1>
		</group>
        
		
		<!-- Demographic -->
		<group ref="/data/demographic">
		
			<input ref="D1">
				<label ref="FBO ID"/>
			</input>
			<input ref="D2">
				<label>Family ID:</label>
			</input>
			<input ref="D3">
				<label>Child ID:</label>
			</input>
			<input ref="D4years">
				<label>Child’s Age in Years:</label>
			</input>
			<input ref="D4months">
				<label>Child’s Age in Months:</label>
			</input>
			
			<select1 ref="D5">
				<label>Attends school or early childhood care:</label>
				<item>
					<label ref="jr:itext('yes')"></label>
					<value>1</value>
				</item>
				<item>
					<label ref="jr:itext('no')"></label>
					<value>0</value>
				</item>
			</select1>
			<input ref="D6">
				<label>If in primary school, which grade:</label>
			</input>
			<input ref="D7">
				<label>Number of children at home:</label>
			</input> 
			<input ref="D8">
				<label>Number of adults at home:</label>
			</input>  
			<select1 ref="D9">
				<label>Your gender:</label>
				<item>
					<label>Male</label>
					<value>1</value>
				</item>
				<item>
					<label>Female</label>
					<value>0</value>
				</item>
			</select1>
			<select1 ref="D10">
				<label>Your relationship with the child:</label>
				<item>
					<label>Mother</label>
					<value>1</value>
				</item>
				<item>
					<label>Father</label>
					<value>2</value>
				</item>
				<item>
					<label>Grandparent</label>
					<value>3</value>
				</item>
				<item>
					<label>Other</label>
					<value>4</value>
				</item>
			</select1>
			<input ref="D10specify" >
				<label>Specify other relationship with the child</label>
			</input>
			<select1 ref="D11">
				<label>What is Your Religion:</label>
				<item>
					<label>No religion</label>
					<value>0</value>
				</item>
				<item>
					<label>Muslim</label>
					<value>1</value>
				</item>
				<item>
					<label>Christian</label>
					<value>2</value>
				</item>
				<item>
					<label>Catholic</label>
					<value>3</value>
				</item>
				<item>
					<label>Other religion </label>
					<value>4</value>
				</item>
			</select1>
			<input ref="D11specify" >
				<label>Specify other religion</label>
			</input>
			<select1 ref="D12">
				<label>Your highest education:</label>
				<item>
					<label>None</label>
					<value>0</value>
				</item>
				<item>
					<label>Primary</label>
					<value>1</value>
				</item>
				<item>
					<label>O’level</label>
					<value>2</value>
				</item>
				<item>
					<label>A’level</label>
					<value>3</value>
				</item>
				<item>
					<label>Tertiary </label>
					<value>4</value>
				</item>
				<item>
					<label>Non Standard Curriculum </label>
					<value>5</value>
				</item>
			</select1>
			<select1 ref="D13">
				<label>Marital Status:</label>
				<item>
					<label>Single (Single/Widow/or Divorced) </label>
					<value>0</value>
				</item>
				<item>
					<label>Married</label>
					<value>1</value>
				</item>
				<item>
					<label>Not married but live with a partner</label>
					<value>2</value>
				</item>
				<item>
					<label>other</label>
					<value>3</value>
				</item>
				</select1>
			<input ref="D13specify" >
				<label>Specify other Marital status</label>
			</input>
		</group>
		
		<!-- Child Social Development -->
		<group ref="/data/empathic-Behaviors">
		<label>In today’s discussion, we will talk about……. your child’s behaviors and relationship; your parenting approaches and strategies; and your family relationship and resources to support your child/children. </label>
		<!-- pictures to display -->
		<input ref="picture1">
			<label ref="jr:itext('pic1')"></label>
			<hint>Child’s behaviors and relationship  </hint>
		</input>
		<input ref="picture2">
			<label ref="jr:itext('pic1')"></label>
			<hint>Parenting approaches</hint>
		</input>
		<input ref="picture2">
			<label ref="jr:itext('pic1')"></label>
			<hint>Family relationship and resources </hint>
		</input>
		
		<select1 ref="q3a">
				<label>1.How often has your child tried to stop a fight or argument </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3b">
				<label>2.How often has your child invited other children to join in a game  </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3c">
				<label>3.How often has your child tried to help someone who has been hurt  </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3d">
				<label>4.How often has your child shared candies and extra food  </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
				<select1 ref="q3e">
				<label>5.How often has your child taken the opportunity to praise the work of less able children  </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3f">
				<label>6.How often has your child shown sympathy to someone who has made a mistake </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3g">
				<label>7.How often has your child offered to help other children who are having difficulty </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3h">
				<label>8.How often has your child helped other children who are feeling sick </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3i">
				<label>9.How often has your child comforted a child who is crying or upset   </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3j">
				<label>10. How often has your child clapped or smiled if someone else does something well </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
				
		</group>
		
		<!-- pear-relationship -->
		<group ref="/data/pear-relationship">
		<label>In the past month, please describe……….</label>
		
		<select1 ref="q4a">
				<label>1.How often your child has been left out by other children </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q4b">
				<label>2.How often other children have refused to let your child play with them </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q4c">
				<label>3.How often your child has not been chosen as a playmate   </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q4d">
				<label>4.How often your child has been actively rejected by other children who refuse to play with him/her  </label>
				<item>
					<label>never</label>
					<value>1</value>
