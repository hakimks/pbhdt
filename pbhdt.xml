<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" 
        xmlns:h="http://www.w3.org/1999/xhtml" 
        xmlns:jr="http://openrosa.org/javarosa" 
        xmlns:orx="http://openrosa.org/xforms" 
        xmlns:odk="http://www.opendatakit.org/xforms"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <h:head>
        <h:title>Pediatric Behavioral Health Digital Toolkit Screening Measures</h:title>
        <model>
			<itext>
				<translation lang="English">
					<text id="yes">
						<value>Yes</value>
					</text>
					<text id="no">
						<value>No</value>
					</text>
					<text id="q1a">
						<value>Your Age:</value>
					</text>
					<text id="qlahint">
						<value>If it's less than 18 years old; Stop here; Only 18 years of age and above is qualified</value>
					</text>
					<text id="q1b">
						<value>Are you the primary caregiver and living with the child?</value>
					</text>
					<text id="qlbhint">
						<value>If NO--> STOP</value>
					</text>
					<text id="q1c">
						<value>Is this the first time you are signing up for using the Digital Toolkit? </value>
					</text>
					<text id ="q2a">
						<value>FBO ID:</value>
					</text>
					
					
				</translation>
				<translation lang="Luganda">
					<text id="yes">
						<value>Yye</value>
					</text>
					<text id="no">
						<value>Nedda</value>
					</text>
					<text id="q1a">
						<value>Emyaaka gyo:</value>
					</text>
					<text id="qlahint">
						<value>Bwegiba tegisukka 18; Koma wano; Emyaka 18 n’okweyongerayo gyokka gye gikkirizibwa </value>
					</text>
					<text id="q1b">
						<value>Gwe alabilira era ng’obeera n’omwana?</value>
					</text>
					<text id="qlbhint">
						<value>Bweba Nedda, Koma Wano</value>
					</text>
					<text id="q1c">
						<value>Guno gwemulundi ogusoose okwewandiisa okukozesa Digital Toolkit? </value>
					</text>
					<text id ="q2a">
						<value>Ennamba y’ekannisa/ekkerezia oba omuzigiti (FBO):</value>
					</text>
					
					
				</translation>
			
			</itext>
			
            <instance>
                <data id="P-BHDT">
					<eligibility>
						<q1a/>
						<q1b/>
						<q1c/>
					</eligibility>
					
					<demographic>
						<q2a/>
						<q2b/>
						<q2c/>
						<q2dyears/>
						<q2dmonths/>
						<q2e/>
						<q2f/>
						<q2g/>
						<q2h/>
						<q2i/>
						<q2j/>
						<q2k/>
						<q2kspecify/>
						<q2l/>
						<q2lspecify/>
						<q2m/>
						<q2n/>
					</demographic>
                    
					<empathic-Behaviors>
						<q3a/>
						<q3b/>
						<q3c/>
						<q3d/>
						<q3e/>
						<q3f/>
						<q3g/>
						<q3h/>
						<q3i/>
						<q3j/>
					</empathic-Behaviors>
					
					<pear-relationship>
						<q4a/>
						<q4b/>
						<q4c/>
						<q4d/>
						<q4e/>
						<q4f/>
						<q4g/>
						<q4h/>
						<q4i/>
					</pear-relationship>	
						
					<family-relationship>
						<q5a/>
						<q5b/>
						<q5c/>
						<q5d/>
						<q5e/>
						<q5f/>
						<q5g/>
						<q5h/>
						<q5i/>
						<q5j/>
					</family-relationship>					
					 
					<DeviceID/>
					<StartTime/>
					<EndTime/>
					<instanceID/>
					<SubscriberID/>
					
				</data>
            </instance>
            <bind nodeset="/data/instanceID" preload="uid" type="xsd:string"/>
			<bind nodeset="/data/StartTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="start"/>
			<bind nodeset="/data/EndTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="end"/>
			<bind nodeset="/data/DeviceID" type="string" jr:preload="property" jr:preloadParams="deviceid"/>
			<bind nodeset="/data/SubscriberID" type="string" jr:preload="property" jr:preloadParams="subscriberid"/>
			<bind nodeset="/data/eligibility/q1a" type="int" required="true()"/>
			<bind nodeset="/data/eligibility/q1b" required="true()" relevant="/data/eligibility/q1a &gt;= 18" />
			<bind nodeset="/data/eligibility/q1c" required="true()" relevant="/data/eligibility/q1a &gt;= 18 and /data/eligibility/q1b = 1 " />
			<bind nodeset="/data/demographic" relevant="/data/eligibility/q1a &gt;= 18 and /data/eligibility/q1b = 1 " />
			<bind nodeset="/data/demographic/q2b" type="int" />
			<bind nodeset="/data/demographic/q2c" type="int" />
			<bind nodeset="/data/demographic/q2dyears" type="int" constraint=". &gt; 0 " jr:constraintMsg="Child’s years can not be 0"/>
			<bind nodeset="/data/demographic/q2dmonths" type="int" constraint=". &gt; 0 " jr:constraintMsg="Child’s Months can not be 0"/>
			<bind nodeset="/data/demographic/q2e" />
			<bind nodeset="/data/demographic/q2f" />
			<bind nodeset="/data/demographic/q2g" />
			<bind nodeset="/data/demographic/q2h" type="int"/>
			<bind nodeset="/data/demographic/q2i" type="int"/>
			<bind nodeset="/data/demographic/q2k" required="true()" />
			<bind nodeset="/data/demographic/q2kspecify" relevant="/data/demographic/q2k = 4" />
			<bind nodeset="/data/demographic/q2l" required="true()" />
			<bind nodeset="/data/demographic/q2lspecify" relevant="/data/demographic/q2l = 4"/>
			<bind nodeset="/data/demographic/q2m" />
			<bind nodeset="/data/demographic/q2n" />
			
			<bind nodeset="/data/empathic-Behaviors" relevant="/data/eligibility/q1a &gt;= 18 and /data/eligibility/q1b = 1 " />
			<bind nodeset="/data/empathic-Behaviors/q3a" />
			<bind nodeset="/data/empathic-Behaviors/q3b" />
			<bind nodeset="/data/empathic-Behaviors/q3c" />
			<bind nodeset="/data/empathic-Behaviors/q3d" />
			<bind nodeset="/data/empathic-Behaviors/q3e" />
			<bind nodeset="/data/empathic-Behaviors/q3f" />
			<bind nodeset="/data/empathic-Behaviors/q3g" />
			<bind nodeset="/data/empathic-Behaviors/q3h" />
			<bind nodeset="/data/empathic-Behaviors/q3i" />
			<bind nodeset="/data/empathic-Behaviors/q3j" />
			
			<bind nodeset="/data/pear-relationship" relevant="/data/eligibility/q1a &gt;= 18 and /data/eligibility/q1b = 1 " />
			<bind nodeset="/data/pear-relationship/q4a" />
			<bind nodeset="/data/pear-relationship/q4b" />
			<bind nodeset="/data/pear-relationship/q4c" />
			<bind nodeset="/data/pear-relationship/q4d" />
			<bind nodeset="/data/pear-relationship/q4e" />
			<bind nodeset="/data/pear-relationship/q4f" />
			<bind nodeset="/data/pear-relationship/q4g" />
			<bind nodeset="/data/pear-relationship/q4h" />
			<bind nodeset="/data/pear-relationship/q4i" />
			
			<bind nodeset="/data/family-relationship" relevant="/data/eligibility/q1a &gt;= 18 and /data/eligibility/q1b = 1 " />
			<bind nodeset="/data/family-relationship/q5a" />
			<bind nodeset="/data/family-relationship/q5b" />
			<bind nodeset="/data/family-relationship/q5c" />
			<bind nodeset="/data/family-relationship/q5d" />
			<bind nodeset="/data/family-relationship/q5e" />
			<bind nodeset="/data/family-relationship/q5f" />
			<bind nodeset="/data/family-relationship/q5g" />
			<bind nodeset="/data/family-relationship/q5h" />
			<bind nodeset="/data/family-relationship/q5i" />
			<bind nodeset="/data/family-relationship/q5j" />
			
			
        </model>
    </h:head>
    <h:body>
		<!-- Eligibility Screening -->
		<group ref="/data/eligibility">
			<input ref="q1a">
				<label ref="jr:itext('q1a')"/>
				<hint ref="jr:itext('qlahint')"></hint>
			</input>
			
			<select1 ref="q1b">
				<label ref="jr:itext('q1b')"></label>
				<hint ref="jr:itext('qlbhint')"></hint>
				<item>
					<label ref="jr:itext('yes')"></label>
					<value>1</value>
				</item>
				<item>
					<label ref="jr:itext('no')"></label>
					<value>0</value>
				</item>
			</select1>
			<select1 ref="q1c">
			<label ref="jr:itext('q1c')"></label>
				<item>
					<label ref="jr:itext('yes')"></label>
					<value>1</value>
				</item>
				<item>
					<label ref="jr:itext('no')"></label>
					<value>0</value>
				</item>
			</select1>
		
		</group>
        
		
		<!-- Demographic -->
		<group ref="/data/demographic">
		
			<input ref="q2a">
				<label ref="jr:itext('q2a')"/>
			</input>
			<input ref="q2b">
				<label>Family ID:</label>
			</input>
			<input ref="q2c">
				<label>Child ID:</label>
			</input>
			<input ref="q2dyears">
				<label>Child’s Age in Years:</label>
			</input>
			<input ref="q2dmonths">
				<label>Child’s Age in Months:</label>
			</input>
			
			<select1 ref="q2e">
				<label>Attends school or early childhood care:</label>
				<item>
					<label>Yes</label>
					<value>1</value>
				</item>
				<item>
					<label>No</label>
					<value>0</value>
				</item>
			</select1>
			
			<input ref="q2f">
			<label>If in primary school, which grade:</label>
			</input>
			<select1 ref="q2g">
				<label>Has the child ever uses the Digital Toolkit before:</label>
				<item>
					<label>Yes</label>
					<value>1</value>
				</item>
				<item>
					<label>No</label>
					<value>0</value>
				</item>
			</select1>
			<input ref="q2h">
			<label>Number of children at home:</label>
			</input> 
			<input ref="q2i">
			<label>Number of adults at home:</label>
			</input>  
			<select1 ref="q2j">
				<label>Your gender:</label>
				<item>
					<label>Male</label>
					<value>1</value>
				</item>
				<item>
					<label>Female</label>
					<value>0</value>
				</item>
			</select1>
			<select1 ref="q2k">
				<label>Your relationship with the child:</label>
				<item>
					<label>Mother</label>
					<value>1</value>
				</item>
				<item>
					<label>Father</label>
					<value>2</value>
				</item>
				<item>
					<label>Grandparent</label>
					<value>3</value>
				</item>
				<item>
					<label>Other</label>
					<value>4</value>
				
				</item>
			
			</select1>
			<input ref="q2kspecify" >
				<label>Specify other relationship with the child</label>
			</input>
			
			<select1 ref="q2l">
				<label>What is Your Religion:</label>
				<item>
					<label>No religion</label>
					<value>0</value>
				</item>
				<item>
					<label>Muslim</label>
					<value>1</value>
				</item>
				<item>
					<label>Christian</label>
					<value>2</value>
				</item>
				<item>
					<label>Catholic</label>
					<value>3</value>
				</item>
				<item>
					<label>Other religion </label>
					<value>4</value>
				</item>
			</select1>
			<input ref="q2lspecify" >
				<label>Specify other religion</label>
			</input>
			<select1 ref="q2m">
				<label>Your highest education:</label>
				<item>
					<label>None</label>
					<value>0</value>
				</item>
				<item>
					<label>Primary</label>
					<value>1</value>
				</item>
				<item>
					<label>O’level</label>
					<value>2</value>
				</item>
				<item>
					<label>A’level</label>
					<value>3</value>
				</item>
				<item>
					<label>Tertiary </label>
					<value>4</value>
				</item>
				<item>
					<label>Non Standard Curriculum </label>
					<value>5</value>
				</item>
			</select1>
			<select1 ref="q2n">
				<label>Maritial Status:</label>
				<item>
					<label>Single (Single/Widow/or Divorced) </label>
					<value>0</value>
				</item>
				<item>
					<label>Married</label>
					<value>1</value>
				</item>
				<item>
					<label>Not married but live with a partner</label>
					<value>2</value>
				</item>
				<item>
					<label>other</label>
					<value>3</value>
				</item>
				</select1>
			
		</group>
		
		<!-- Child Social Development -->
		<group ref="/data/empathic-Behaviors">
		<label>In the past month, please describe……….</label>
		
		<select1 ref="q3a">
				<label>1.How often has your child tried to stop a fight or argument </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3b">
				<label>2.How often has your child invited other children to join in a game  </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3c">
				<label>3.How often has your child tried to help someone who has been hurt  </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3d">
				<label>4.How often has your child shared candies and extra food  </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
				<select1 ref="q3e">
				<label>5.How often has your child taken the opportunity to praise the work of less able children  </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3f">
				<label>6.How often has your child shown sympathy to someone who has made a mistake </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3g">
				<label>7.How often has your child offered to help other children who are having difficulty </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3h">
				<label>8.How often has your child helped other children who are feeling sick </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3i">
				<label>9.How often has your child comforted a child who is crying or upset   </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q3j">
				<label>10. How often has your child clapped or smiled if someone else does something well </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
				
		</group>
		
		<!-- pear-relationship -->
		<group ref="/data/pear-relationship">
		<label>In the past month, please describe……….</label>
		
		<select1 ref="q4a">
				<label>1.How often your child has been left out by other children </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q4b">
				<label>2.How often other children have refused to let your child play with them </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q4c">
				<label>3.How often your child has not been chosen as a playmate   </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
					<label>sometimes</label>
					<value>3</value>
				</item>
				<item>
					<label>usually</label>
					<value>4</value>
				</item>
				<item>
					<label>always</label>
					<value>5</value>
				</item>
			</select1>
			
			<select1 ref="q4d">
				<label>4.How often your child has been actively rejected by other children who refuse to play with him/her  </label>
				<item>
					<label>never</label>
					<value>1</value>
				</item>
				<item>
					<label>rarely</label>
					<value>2</value>
				</item>
				<item>
