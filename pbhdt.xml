<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" 
        xmlns:h="http://www.w3.org/1999/xhtml" 
        xmlns:jr="http://openrosa.org/javarosa" 
        xmlns:orx="http://openrosa.org/xforms" 
        xmlns:odk="http://www.opendatakit.org/xforms"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <h:head>
        <h:title>Pediatric Behavioral Health Digital Toolkit Screening Measures</h:title>
        <model>
			<itext>
				<translation default="true()" lang="English">
					<text id="yes">
						<value>Yes</value>
					</text>
					<text id="no">
						<value>No</value>
					</text>
					<text id="pic1">
						<value form="image">jr://images/pic1.jpg</value>
					</text>
					<text id="pic2">
						<value form="image">jr://images/pic2.jpg</value>
					</text>
					<text id="pic3">
						<value form="image">jr://images/pic2.jpg</value>
					</text>
					<text id="developmenttree">
						<value form="image">jr://images/developmenttree.jpg</value>
					</text>
				</translation>
				<translation lang="Luganda">
					
				</translation>
			
			</itext>
			
            <instance>
                <data id="P-BHDT">
					<eligibility>
						<P_Age/>
						<PCaregiver/>
						<ToolN/>
					</eligibility>
					
					<demographic>
						<FBO_ID/>
						<FamID/>
						<ChildID/>
						<C_Age_Years/>
						<C_Age_Months/>
						<ECC_Attend/>
						<CGrade/>
						<ChildN/>
						<AdultN/>
						<P.Gender/>
						<RelChild/>
						<RelChild_Specify/>
						<Religion/>
						<Religion_Specify/>
						<P_Edu/>
						<MaritalS/>
						<MaritalS_Specify/>
					</demographic>
					<vignette>
						<childV/>
						<ParentingV/>
						<FamV/>
					</vignette>
					<tree/>
					<Social>
						<S_EmpBeh>
							<S_EmpBeh_Description/>
							<S_EmpBeh-header/>
							<S_EmpBeh_1/>
							<S_EmpBeh_2/>
							<S_EmpBeh_3/>
							<S_EmpBeh_4/>
							<S_EmpBeh_5/>
						</S_EmpBeh>
						<S_PeerRel>
							<S_PeerRel_Description/>
							<S_PeerRel-header/>
							<S_PeerRel_1/>
							<S_PeerRel_2/>
							<S_PeerRel_3/>
							<S_PeerRel_4/>
						</S_PeerRel>
						<S_FamRel>
							<S_FamRel_Description/>
							<S_FamRel-header/>
							<S_FamRel_1/>
							<S_FamRel_2/>
							<S_FamRel_3/>
							<S_FamRel_4/>
							<S_FamRel_5/>
							<S_FamRel_6/>
							<S_FamRel_7/>
							<S_FamRel_8/>
						</S_FamRel>
					</Social>
					<Emotional>	
						<E_Anger>
							<E_Anger_Description/>
							<E_Anger-header/>
							<E_Anger_1/>
							<E_Anger_2/>
							<E_Anger_3/>
							<E_Anger_4/>
							<E_Anger_5/>
							<E_Anger_6/>
							<E_Anger_7/>
							<E_Anger_8/>
							<E_Anger_9/>					
						</E_Anger>
						<E_OvAnx>
							<E_OvAnx_Description/>
							<E_OvAnx-header/>
							<E_OvAnx_1/>
							<E_OvAnx_2/>
							<E_OvAnx_3/>
							<E_OvAnx_4/>
							<E_OvAnx_5/>
							<E_OvAnx_6/>
						</E_OvAnx>
						<E_Sad>
							<E_Sad_description/>
							<E_Sad-header/>
							<E_Sad_1/>
							<E_Sad_2/>
							<E_Sad_3/>
							<E_Sad_4/>
							<E_Sad_5/>
							<E_Sad_6/>
							<E_Sad_7/>
						</E_Sad>
					
					</Emotional>
					
					<meta>
						<DeviceID/>
						<StartTime/>
						<EndTime/>
						<instanceID/>
						<SubscriberID/>
						<phoneNumber/>
					</meta>
					
					
					
				</data>
            </instance>
            <bind nodeset="/data/meta/instanceID" preload="uid" type="xsd:string"/>
			<bind nodeset="/data/meta/StartTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="start"/>
			<bind nodeset="/data/meta/EndTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="end"/>
			<bind nodeset="/data/meta/DeviceID" type="string" jr:preload="property" jr:preloadParams="deviceid"/>
			<bind nodeset="/data/meta/SubscriberID" type="string" jr:preload="property" jr:preloadParams="subscriberid"/>
			<bind nodeset="/data/meta/phoneNumber" type="string" jr:preload="property" jr:preloadParams="phone number"/>
			
			<bind nodeset="/data/eligibility/P_Age" type="int" required="true()"/>
			<bind nodeset="/data/eligibility/PCaregiver" required="true()" relevant="/data/eligibility/P_Age &gt;= 18" />
			<bind nodeset="/data/eligibility/ToolN" required="true()" relevant="/data/eligibility/P_Age &gt;= 18 and /data/eligibility/PCaregiver = 1 " />
			
			<bind nodeset="/data/demographic" relevant="/data/eligibility/P_Age &gt;= 18 and /data/eligibility/PCaregiver = 1 " />
			<bind nodeset="/data/demographic/FamID" type="int" />
			<bind nodeset="/data/demographic/ChildID" type="int" />
			<bind nodeset="/data/demographic/C_Age_Years" type="int" constraint=". &gt; 0 " jr:constraintMsg="Child’s years can not be 0"/>
			<bind nodeset="/data/demographic/C_Age_Months" type="int" constraint=". &gt; 0 " jr:constraintMsg="Child’s Months can not be 0"/>
			<bind nodeset="/data/demographic/ECC_Attend" required="true()" />
			<bind nodeset="/data/demographic/CGrade" relevant="/data/demographic/ECC_Attend = 1" />
			<bind nodeset="/data/demographic/ChildN" type="int"/>
			<bind nodeset="/data/demographic/AdultN" type="int"/>
			<bind nodeset="/data/demographic/RelChild" />
			<bind nodeset="/data/demographic/RelChild_Specify" relevant="/data/demographic/RelChild = 4" />
			<bind nodeset="/data/demographic/Religion" />
			<bind nodeset="/data/demographic/Religion_Specify" relevant="/data/demographic/Religion = 4"/>
			<bind nodeset="/data/demographic/P_Edu" />
			<bind nodeset="/data/demographic/MaritalS" />
			<bind nodeset="/data/demographic/MaritalS_Specify" relevant="/data/dedication/MaritalS = 3"/>
			
			
			<bind nodeset="/data/vignette/childV" readonly="true()" relevant="/data/eligibility/P_Age &gt;= 18 and /data/eligibility/PCaregiver = 1 " />
			<bind nodeset="/data/vignette/ParentingV" readonly="true()" relevant="/data/eligibility/P_Age &gt;= 18 and /data/eligibility/PCaregiver = 1 " />
			<bind nodeset="/data/vignette/FamV" readonly="true()" relevant="/data/eligibility/P_Age &gt;= 18 and /data/eligibility/PCaregiver = 1 " />
			<bind nodeset="/data/tree" readonly="true()" relevant="/data/eligibility/P_Age &gt;= 18 and /data/eligibility/PCaregiver = 1 " />
			
			<bind nodeset="/data/Social" relevant="/data/eligibility/P_Age &gt;= 18 and /data/eligibility/PCaregiver = 1 " />
			<bind nodeset="/data/Social/S_EmpBeh/S_EmpBeh_Description" readonly="true()" />
			<bind nodeset="/data/Social/S_EmpBeh/S_EmpBeh_1" />
			<bind nodeset="/data/Social/S_EmpBeh/S_EmpBeh_2" />
			<bind nodeset="/data/Social/S_EmpBeh/S_EmpBeh_3" />
			<bind nodeset="/data/Social/S_EmpBeh/S_EmpBeh_4" />
			<bind nodeset="/data/Social/S_EmpBeh/S_EmpBeh_5" />	
			<bind nodeset="/data/Social/S_PeerRel" />
			<bind nodeset="/data/Social/S_PeerRel/S_PeerRel_Description"  readonly="true()"/>
			<bind nodeset="/data/Social/S_FamRel" />
			<bind nodeset="/data/Social/S_FamRel/S_FamRel_Description" readonly="true()" />
			
			
			<bind nodeset="/data/Emotional" relevant="/data/eligibility/P_Age &gt;= 18 and /data/eligibility/PCaregiver = 1 " />
			<bind nodeset="/data/Emotional/E_Anger"/>
			<bind nodeset="/data/Emotional/E_Anger/E_Anger_Description" readonly="true()"/>
			<bind nodeset="/data/Emotional/E_OvAnx" />
			<bind nodeset="/data/Emotional/E_OvAnx/E_OvAnx_Description" readonly="true()" />
			<bind nodeset="/data/Emotional/E_Sad" />
			<bind nodeset="/data/Emotional/E_Sad/E_Sad_description" readonly="true()" />
			
			
        </model>
    </h:head>
    <h:body>
		<!-- Eligibility Screening -->
		<group ref="/data/eligibility" appearance="field-list">
			<input ref="P_Age">
				<label>Your Age</label>
				<hint>If less than 18 years old; Stop here; Only greater than or equal to 18 years of age is qualified"></hint>
			</input>
			
			<select1 ref="PCaregiver">
				<label>Are you the primary caregiver and living with the child?"</label>
				<hint>If NO, STOP"></hint>
				<item>
					<label ref="jr:itext('yes')"></label>
					<value>1</value>
				</item>
				<item>
					<label ref="jr:itext('no')"></label>
					<value>0</value>
				</item>
			</select1>
			<select1 ref="ToolN">
			<label>Is this the first time you are signing up for using the Digital Toolkit?</label>
				<item>
					<label ref="jr:itext('yes')"></label>
					<value>1</value>
				</item>
				<item>
					<label ref="jr:itext('no')"></label>
					<value>0</value>
				</item>
			</select1>
		</group>
        
		
		<!-- Demographic -->
		<group ref="/data/demographic" appearance="field-list">
		
			<input ref="FBO_ID">
				<label>FBO ID:</label>
			</input>
			<input ref="FamID">
				<label>Family ID:</label>
			</input>
			<input ref="ChildID">
				<label>Child ID:</label>
			</input>
			<input ref="C_Age_Years">
				<label>Child’s Age in Years:</label>
			</input>
			<input ref="C_Age_Months">
				<label>Child’s Age in Months:</label>
			</input>
			
			<select1 ref="ECC_Attend">
				<label>Attend school or early childhood care:</label>
				<item><label ref="jr:itext('yes')"></label><value>1</value></item>
				<item><label ref="jr:itext('no')"></label><value>0</value>
				</item>
			</select1>
			<input ref="CGrade">
				<label>If in primary school, which grade:</label>
			</input>
			<input ref="ChildN">
				<label>Number of children at home:</label>
			</input> 
			<input ref="AdultN">
				<label>Number of adults at home:</label>
			</input>  
			<select1 ref="P.Gender">
				<label>Your gender:</label>
				<item><label>Male</label><value>1</value></item>
				<item><label>Female</label><value>0</value></item>
			</select1>
			<select1 ref="RelChild">
				<label>Your relationship with the child:</label>
				<item><label>Mother</label><value>1</value></item>
				<item><label>Father</label><value>2</value></item>
				<item><label>Grandparent</label><value>3</value></item>
				<item><label>Other</label><value>4</value></item>
			</select1>
			<input ref="RelChild_Specify" >
				<label>Specify other relationship with the child</label>
			</input>
			<select1 ref="Religion">
				<label>What is Your Religion:</label>
				<item><label>No religion</label><value>0</value></item>
				<item><label>Muslim</label><value>1</value></item>
				<item><label>Christian</label><value>2</value></item>
				<item><label>Catholic</label><value>3</value></item>
				<item><label>Other Religion </label><value>4</value></item>
			</select1>
			<input ref="Religion_Specify" >
				<label>Specify other Religion</label>
			</input>
			<select1 ref="P_Edu">
				<label>Your highest education:</label>
				<item><label>None</label><value>0</value></item>
				<item><label>Primary</label><value>1</value></item>
				<item><label>O’level</label><value>2</value></item>
				<item><label>A’level</label><value>3</value></item>
				<item><label>Tertiary </label><value>4</value></item>
				<item><label>Non Standard Curriculum </label><value>5</value></item>
			</select1>
			<select1 ref="MaritalS">
				<label>Marital Status:</label>
				<item><label>Single (Single/Widow/or Divorced) </label><value>0</value></item>
				<item><label>Married</label><value>1</value></item>
				<item><label>Not married but live with a partner</label><value>2</value></item>
				<item><label>other</label><value>3</value></item>
				</select1>
			<input ref="MaritalS_Specify" >
				<label>Specify other Marital status</label>
			</input>
		</group>
		<!-- Child Social Development -->
			<group ref="vignette" appearance="field-list">
				<label>In today’s discussion, we will talk about……. your child’s behaviors and relationship; your parenting approaches and strategies; and your family relationship and resources to support your child/children. </label>
				<!-- pictures to display -->
				<input ref="childV">
					<hint>Child’s behaviors and relationship  </hint>
					<label ref="jr:itext('pic1')"></label>
				</input>
				<input ref="ParentingV">
					<hint>Parenting approaches</hint>
					<label ref="jr:itext('pic1')"></label>
				</input>
				<input ref="FamV">
					<hint>Family relationship and resources </hint>
					<label ref="jr:itext('pic1')"></label>
				</input>
			</group>
			<input ref="tree">
				<hint>A child’s growth is like a tree. There are five areas of child development (including social, emotional, behavioral, physical, and academic/learning). To support a strong tree, the child needs to have adequate nutrition, which is usually provided by parents and family members. All the areas listed in this picture are inter-related. For this part of the interview, I am going to ask you questions from each area. We will look at the results together after you have answered all the questions</hint>
				<label ref="jr:itext('developmenttree')"></label>
			</input>
	<!-- Child Social -->
		<group ref="/data/Social">	
			<group ref="S_EmpBeh" appearance="field-list">
				<input ref="S_EmpBeh_Description">
					<label>In the past month, please describe………</label>
					<hint>1=Never, 2=Rarely, 3=Sometimes, 4=Usually, 5=Always</hint>
				</input>
				<select1 appearance="label" ref="S_EmpBeh-header">
					<label></label>
					<item><label>Never</label><value>1</value></item>
					<item><label>Rarely</label><value>2</value></item>
					<item><label>Sometimes</label><value>3</value></item>
					<item><label>Usually</label><value>4</value></item>
					<item><label>Always</label><value>5</value></item>
				</select1>
				
				<select1 appearance="list-nolabel" ref="S_EmpBeh_1">
					<label>How often has your child tried to stop a fight or argument </label>
					<item><label>Never</label><value>1</value></item>
					<item><label>Rarely</label><value>2</value></item>
					<item><label>Sometimes</label><value>3</value></item>
					<item><label>Usually</label><value>4</value></item>
					<item><label>Always</label><value>5</value></item>
				</select1>
			
				<select1 appearance="list-nolabel" ref="S_EmpBeh_2">
					<label>How often has your child shared candies and extra food  </label>
					<item><label>Never</label><value>1</value></item>
					<item><label>Rarely</label><value>2</value></item>
					<item><label>Sometimes</label><value>3</value></item>
					<item><label>Usually</label><value>4</value></item>
					<item><label>Always</label><value>5</value></item>
				</select1>
				
				<select1 appearance="list-nolabel" ref="S_EmpBeh_3">
					<label>How often has your child helped other children who are feeling sick </label>
					<item><label>Never</label><value>1</value></item>
					<item><label>Rarely</label><value>2</value></item>
					<item><label>Sometimes</label><value>3</value></item>
					<item><label>Usually</label><value>4</value></item>
					<item><label>Always</label><value>5</value></item>
				</select1>
				
				<select1 appearance="list-nolabel" ref="S_EmpBeh_4">
					<label>How often has your child comforted a child who is crying or upset  </label>
					<item><label>Never</label><value>1</value></item>
					<item><label>Rarely</label><value>2</value></item>
					<item><label>Sometimes</label><value>3</value></item>
					<item><label>Usually</label><value>4</value></item>
					<item><label>Always</label><value>5</value></item>
				</select1>
			
				<select1 appearance="list-nolabel" ref="S_EmpBeh_5">
					<label>How often has your child clapped or smiled if someone else does something well  </label>
					<item><label>Never</label><value>1</value></item>
					<item><label>Rarely</label><value>2</value></item>
					<item><label>Sometimes</label><value>3</value></item>
					<item><label>Usually</label><value>4</value></item>
					<item><label>Always</label><value>5</value></item>
				</select1>
			</group>
			<!-- Peer Relationships -->
			<group ref="S_PeerRel" appearance="field-list">
				<input ref="S_PeerRel_Description">
					<label>In the past month, please describe……….</label>
					<hint>1=Never, 2=Rarely, 3=Sometimes, 4=Usually, 5=Always</hint>
				</input>
				<select1 appearance="label" ref="S_PeerRel-header">
					<label></label>
					<item><label>Never</label><value>1</value></item>
					<item><label>Rarely</label><value>2</value></item>
					<item><label>Sometimes</label><value>3</value></item>
					<item><label>Usually</label><value>4</value></item>
					<item><label>Always</label><value>5</value></item>
				</select1>
				<select1 appearance="list-nolabel" ref="S_PeerRel_1">
					<label>How often your child has been left out by other children </label>
					<item><label>Never</label><value>1</value></item>
					<item><label>Rarely</label><value>2</value></item>
					<item><label>Sometimes</label><value>3</value></item>
					<item><label>Usually</label><value>4</value></item>
					<item><label>Always</label><value>5</value></item>
				</select1>
				
				<select1 appearance="list-nolabel" ref="S_PeerRel_2">
					<label>How often other children have refused to let your child play with them </label>
					<item><label>Never</label><value>1</value></item>
					<item><label>Rarely</label><value>2</value></item>
					<item><label>Sometimes</label><value>3</value></item>
					<item><label>Usually</label><value>4</value></item>
					<item><label>Always</label><value>5</value></item>
				</select1>
				
				<select1 appearance="list-nolabel" ref="S_PeerRel_3">
					<label>How often your child has been teased and made fun of by other children  </label>
					<item><label>Never</label><value>1</value></item>
					<item><label>Rarely</label><value>2</value></item>
					<item><label>Sometimes</label><value>3</value></item>
					<item><label>Usually</label><value>4</value></item>
					<item><label>Always</label><value>5</value></item>
				</select1>
				
				<select1 appearance="list-nolabel" ref="S_PeerRel_4">
					<label>How often your child has been pushed or shoved around by other children </label>
					<item><label>Never</label><value>1</value></item>
					<item><label>Rarely</label><value>2</value></item>
					<item><label>Sometimes</label><value>3</value></item>
					<item><label>Usually</label><value>4</value></item>
					<item><label>Always</label><value>5</value></item>
				</select1>
			</group> 
			<!-- Family Relationship  -->
			<group ref="S_FamRel" appearance="field-list" >
				<input ref="S_FamRel_Description">
					<label>For each of the following statement, please tell me how much it is like your child…</label>
					<hint>0 = Does not do this at all, 1 = Does this a little, 2 = Does this quite a bit, 3 = Does this a lot</hint>
				</input>
				<select1 appearance="label" ref="S_FamRel-header">
					<label></label>
					<item><label>Does not do this at all</label><value>0</value></item>
					<item><label>Does this a little</label><value>1</value></item>
					<item><label>Does this quite a bit</label><value>2</value></item>
					<item><label>Does this a lot</label><value>3</value></item>
				</select1>
				<select1 appearance="list-nolabel" ref="S_FamRel_1">
					<label>Participates in family activities </label>
					<item><label>Does not do this at all</label><value>0</value></item>
					<item><label>Does this a little</label><value>1</value></item>
					<item><label>Does this quite a bit</label><value>2</value></item>
					<item><label>Does this a lot</label><value>3</value></item>
					</select1>
					
					
				<select1 appearance="list-nolabel" ref="S_FamRel_2">
					<label>Complies with rules at home </label>
					<item><label>Does not do this at all</label><value>0</value></item>
					<item><label>Does this a little</label><value>1</value></item>
					<item><label>Does this quite a bit</label><value>2</value></item>
					<item><label>Does this a lot</label><value>3</value></item>
					</select1>
							
				<select1 appearance="list-nolabel" ref="S_FamRel_3">
					<label>Participates in church or religion activities </label>
					<item><label>Does not do this at all</label><value>0</value></item>
					<item><label>Does this a little</label><value>1</value></item>
					<item><label>Does this quite a bit</label><value>2</value></item>
					<item><label>Does this a lot</label><value>3</value></item>
				</select1>
				<select1 appearance="list-nolabel" ref="S_FamRel_4">
					<label>Interacts positively with siblings</label>
					<item><label>Does not do this at all</label><value>0</value></item>
					<item><label>Does this a little</label><value>1</value></item>
					<item><label>Does this quite a bit</label><value>2</value></item>
					<item><label>Does this a lot</label><value>3</value></item>
				</select1>
				<select1 appearance="list-nolabel" ref="S_FamRel_5">
					<label>Interacts positively with parents </label>
					<item><label>Does not do this at all</label><value>0</value></item>
					<item><label>Does this a little</label><value>1</value></item>
					<item><label>Does this quite a bit</label><value>2</value></item>
					<item><label>Does this a lot</label><value>3</value></item>
					</select1>
				<select1 appearance="list-nolabel" ref="S_FamRel_6">
					<label>Trust a significant person with his or her life </label>
					<item><label>Does not do this at all</label><value>0</value></item>
